{
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.Async"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning",
        "Hangfire": "Information",
        "Npgsql": "Warning",
        "Relisten": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
              }
            }
          ]
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId", "WithProcessId", "WithEnvironmentName"],
    "Properties": {
      "Application": "RelistenApi"
    }
  },
  "REDIS_URL": "redis://127.0.0.1:16379",
  "DATABASE_URL": "postgresql://relisten:local_dev_password@localhost:15432/relisten_db",
  "PGBOUNCER_DATABASE_URL": "postgresql://relisten:local_dev_password@localhost:16432/relisten_db",
  "ASPNETCORE_ENVIRONMENT": "Development",
  "PORT": 3823,
  "PANIC_KEY": "abc123",
  "PHISHIN_KEY": "d2eec9e10b100af4329e0f748b0dd8f728089a77028a7531f061206c5db2e6bcb74447bf81011024a14de9702ffd510d",
  "RELISTEN_ADMIN": {
